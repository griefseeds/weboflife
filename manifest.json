{
  "name": "Web Of Life",
  "description": "Creates Conway's Game of Life!",
  "version": "0.2",
  "content_scripts": [
    {
     "js": ["html2canvas.js"],
     "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "./popup/popup.html",
    "default_title": "Web of Life"
  },
  "manifest_version": 2
}

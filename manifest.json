{
  "name": "Web Of Life",
  "description": "Creates Conway's Game of Life!",
  "version": "0.1",
  "content_scripts": [
    {
     "run_at": "document_end",
     "js": ["content.js", "html2canvas.js"],
     "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "./popup/popup.html",
    "default_title": "yeah dude"
  },
  "manifest_version": 2
}
